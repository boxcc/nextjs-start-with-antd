{
  "name": "demo",
  "description": "",
  "version": "0.0.1",
  "private": true,
  "author": "",
  "license": "UNLICENSED",
  "scripts": {
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "sver": "standard-version",
    "dev": "next dev",
    "build": "next build",
    "postbuild": "next-sitemap",
    "start": "next start",
    "type-check": "tsc",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "postinstall": "husky install",
    "commit": "cz",
    "pm2": "cross-env NODE_LOG_DIR=/tmp ENABLE_NODE_LOG=YES pm2 start pm2.json",
    "pm2:restart": "cross-env NODE_LOG_DIR=/tmp ENABLE_NODE_LOG=YES pm2 restart pm2.json",
    "codegen": "cross-env DOTENV_CONFIG_PATH=./.env.local graphql-codegen --config codegen.yml --require dotenv/config",
    "release": "node ./scripts/release"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "yarn lint --fix",
      "yarn format"
    ]
  },
  "dependencies": {
    "@ant-design/cssinjs": "1.5.6",
