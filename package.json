{
  "name": "demo",
  "description": "",
  "version": "0.0.1",
  "private": true,
  "author": "",
  "license": "UNLICENSED",
  "scripts": {
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "release:major": "standard-version --release-as major",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "postinstall": "husky install",
    "commit": "cz",
    "pm2": "cross-env NODE_LOG_DIR=/tmp ENABLE_NODE_LOG=YES pm2 start pm2.json",
    "pm2:restart": "cross-env NODE_LOG_DIR=/tmp ENABLE_NODE_LOG=YES pm2 restart pm2.json",
    "codegen": "cross-env DOTENV_CONFIG_PATH=./.env.local graphql-codegen --config codegen.yml --require dotenv/config"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "yarn lint --fix",
      "yarn format"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "4.7.0",
    "ahooks": "3.7.0",
    "antd": "4.22.4",
    "classnames": "2.3.1",
    "cross-env": "7.0.3",
    "express": "4.18.1",
    "fetch-jsonp": "1.2.1",
    "next": "12.2.4",
    "next-plugin-antd-less": "1.8.0",
    "next-seo": "5.5.0",
    "nookies": "2.5.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-query": "3.39.1"
  },
  "devDependencies": {
    "@commitlint/cli": "17.0.3",
    "@commitlint/config-conventional": "17.0.3",
    "@fullhuman/postcss-purgecss": "4.1.3",
    "@graphql-codegen/cli": "2.8.0",
    "@graphql-codegen/introspection": "2.2.0",
    "@graphql-codegen/typescript": "2.7.1",
    "@graphql-codegen/typescript-operations": "2.5.1",
    "@graphql-codegen/typescript-react-query": "3.6.1",
    "@next/bundle-analyzer": "12.2.4",
    "@types/node": "14.17.0",
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "5.8.0",
    "@typescript-eslint/parser": "5.8.0",
    "babel-plugin-import": "1.13.5",
    "babel-plugin-module-resolver": "4.1.0",
    "commitizen": "4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.5.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-typescript": "2.5.0",
    "eslint-plugin-import": "2.23.3",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.28.0",
    "eslint-plugin-react-hooks": "4.3.0",
    "husky": "8.0.1",
    "lint-staged": "12.1.3",
    "postcss": "8.4.16",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-preset-env": "7.7.2",
    "prettier": "2.5.1",
    "standard-version": "9.3.0",
    "typescript": "4.5.4",
    "webpack": "5.72.1"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "resolutions": {
    "rc-overflow": "1.2.6"
  }
}
